@model IEnumerable<OdeToFood.Data.Models.Restaurant>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1> ASP.NET</h1>
    @DateTime.Now.Year;

    <div>
        There are @Model.Count() restaurrnats in the db.
    </div>
</div>

<ul>
    @foreach (var restaurant in Model)
    {
    <li> @restaurant.Name</li>
    }
     
</ul>